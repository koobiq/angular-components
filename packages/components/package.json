{
    "name": "@koobiq/components",
    "version": "{{VERSION}}",
    "description": "koobiq",
    "repository": {
        "type": "git",
        "url": "https://github.com/koobiq/angular-components.git"
    },
    "keywords": [
        "angular",
        "component",
        "service",
        "directive",
        "development",
        "kit",
        "koobiq"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/koobiq/angular-components/issues"
    },
    "homepage": "https://github.com/koobiq/angular-components#readme",
    "peerDependencies": {
        "@angular/animations": "{{NG_VERSION}}",
        "@angular/cdk": "{{NG_VERSION}}",
        "@angular/core": "{{NG_VERSION}}",
        "@angular/forms": "{{NG_VERSION}}",
        "@koobiq/cdk": "{{VERSION}}",
        "@koobiq/angular-moment-adapter": "{{VERSION}}",
        "@koobiq/angular-luxon-adapter": "{{VERSION}}",
        "@koobiq/date-formatter": "^3.2.3",
        "@koobiq/icons": "^11.1.3",
        "@koobiq/tokens-builder": "3.11.0",
        "@koobiq/design-tokens": "3.14.0"
    },
    "dependencies": {
        "tslib": "^2.6.2"
    },
    "ng-update": {
        "packageGroup": [
            "@koobiq/components"
        ]
    },
    "exports": {
        ".": {
            "sass": "./_index.scss"
        },
        "./prebuilt-themes/light-theme.css": {
            "style": "./prebuilt-themes/light-theme.css"
        },
        "./prebuilt-themes/dark-theme.css": {
            "style": "./prebuilt-themes/dark-theme.css"
        },
        "./prebuilt-themes/theme.css": {
            "style": "./prebuilt-themes/theme.css"
        },
        "./prebuilt-visual/default-visual.css": {
            "style": "./prebuilt-visual/default-visual.css"
        }
    },
    "schematics": "./schematics/collection.json",
    "sideEffects": false
}
