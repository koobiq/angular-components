        "release:extract-changelog": "ts-node --project tools/extract-release-notes/tsconfig.json tools/extract-release-notes",
        "release:stage": "ts-node --project packages/cli/tsconfig.lib.json packages/cli/src/cli stage",
        "release:stage:commit": "ts-node --project packages/cli/tsconfig.lib.json packages/cli/src/cli stage-commit",
        "release:publish:ci": "ts-node --project packages/cli/tsconfig.lib.json packages/cli/src/cli publish-ci",
        "release:publish:dist": "ts-node --project p packages/cli/tsconfig.lib.json packages/cli/src/cli publish-dist",
        "release:publish:ci-gitlab": "ts-node --project packages/cli/tsconfig.lib.json packages/cli/src/cli publish-ci-gitlab",
        "release:publish:ci-github": "ts-node --project packages/cli/tsconfig.lib.json packages/cli/src/cli publish-ci-github",
